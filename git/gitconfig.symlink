[core]
    eol = lf
    fileMode = false
[credential]
    helper = cache --timeout=84600000
[push]
    default = tracking
[color]
    ui = true

# include data not shared in my dotfiles
[include]
    path = ~/.gitconfig.include
[merge]
    tool = vimdiff
[url "https://github.com/"]
	insteadOf = git@github.com:
[url "https://"]
	insteadOf = git://
